        "@microsoft/applicationinsights-web": "^3.3.7",
        "@mtnvencenzo/kelso-component-library": "^1.0.4",
        "@mui/icons-material": "^7.1.0",
        "@mui/material": "^7.1.0",
        "@testing-library/dom": "^10.4.0",
        "countries-list": "^3.1.1",
        "lodash": "^4.17.21",
        "react": "^19.1.0",
        "react-avatar-editor": "^13.0.2",
