{
    "name": "cocktails-web",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "loc": "chmod +x ./env.sh && ./env.sh .env.loc && vite",
        "cy": "chmod +x ./env.sh && ./env.sh .env.cypress && vite",
        "test": "vitest",
        "coverage": "vitest run --coverage",
        "testui": "vitest --ui --coverage.enabled=true",
        "build:prod": "tsc -p tsconfig.prod.json && vite build --mode prod",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "nswag-cocktails-api": "nswag run src/api/nswagConfig.json /variables:SpecUrl=https://localhost:7176/scalar/v1/openapi.json,Class=CocktailsApiClient,ExceptionClass=CocktailsApiClientException,BaseClass=CocktailsApiClientBase,Folder=cocktailsApi,ExtensionFile=cocktailsApiClientExtensions.ts,OutputFile=cocktailsApiClient.ts"
    },
    "dependencies": {
        "@azure/msal-browser": "^4.12.0",
        "@azure/msal-react": "^3.0.12",
        "@emotion/react": "^11.14.0",
