{
    "name": "cocktails-web",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "loc": "chmod +x ./env.sh && ./env.sh .env.loc && vite",
        "cy": "chmod +x ./env.sh && ./env.sh .env.cypress && vite",
        "test": "vitest",
        "coverage": "vitest run --coverage",
        "testui": "vitest --ui --coverage.enabled=true",
        "build:prod": "tsc -p tsconfig.prod.json && vite build --mode prod",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "nswag-cocktails-api": "nswag run src/api/nswagConfig.json /variables:SpecUrl=http://localhost:7177/scalar/v1/openapi.json,Class=CocktailsApiClient,ExceptionClass=CocktailsApiClientException,BaseClass=CocktailsApiClientBase,Folder=cocktailsApi,ExtensionFile=cocktailsApiClientExtensions.ts,OutputFile=cocktailsApiClient.ts",
        "analyze": "vite-bundle-visualizer --template=treemap --open"
    },
    "dependencies": {
        "@auth0/auth0-spa-js": "^2.4.1",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@mtnvencenzo/kelso-component-library": "^1.0.4",
        "@mui/icons-material": "^7.3.4",
        "@mui/material": "^7.3.4",
        "@opentelemetry/api": "^1.9.0",
        "@opentelemetry/api-logs": "^0.205.0",
        "@opentelemetry/context-zone": "^2.1.0",
        "@opentelemetry/exporter-logs-otlp-http": "^0.205.0",
        "@opentelemetry/exporter-trace-otlp-http": "^0.205.0",
        "@opentelemetry/instrumentation": "^0.205.0",
        "@opentelemetry/instrumentation-document-load": "^0.50.0",
        "@opentelemetry/instrumentation-fetch": "^0.205.0",
        "@opentelemetry/instrumentation-user-interaction": "^0.50.0",
        "@opentelemetry/instrumentation-xml-http-request": "^0.205.0",
        "@opentelemetry/resources": "^2.1.0",
        "@opentelemetry/sdk-logs": "^0.205.0",
        "@opentelemetry/sdk-trace-base": "^2.1.0",
        "@opentelemetry/sdk-trace-web": "^2.1.0",
        "@opentelemetry/semantic-conventions": "^1.37.0",
        "countries-list": "^3.1.1",
        "lodash-es": "^4.17.21",
        "react": "^19.2.0",
        "react-avatar-editor": "^13.0.2",
        "react-dom": "^19.2.0",
        "react-google-recaptcha": "^3.1.0",
        "react-image-file-resizer": "^0.4.8",
        "react-infinite-scroll-component": "^6.1.0",
        "react-markdown": "^10.1.0",
        "react-router-dom": "^7.9.3",
        "react-toastify": "^11.0.5",
        "react-tooltip": "^5.29.1",
        "rehype-raw": "^7.0.0"
    },
    "devDependencies": {
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/lodash": "^4.17.20",
        "@types/node": "^22.15.17",
        "@types/react": "^19.2.0",
        "@types/react-avatar-editor": "^13.0.4",
        "@types/react-dom": "^19.2.0",
        "@types/react-google-recaptcha": "^2.1.9",
        "@typescript-eslint/eslint-plugin": "^8.46.0",
        "@typescript-eslint/parser": "^7.0.1",
        "@vitejs/plugin-react": "^5.0.4",
        "@vitest/coverage-istanbul": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "eslint": "^8.56.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-prettier": "^5.5.0",
        "eslint-plugin-react": "7.37.5",
        "eslint-plugin-react-hooks": "6.1.0",
        "eslint-plugin-react-refresh": "^0.4.23",
        "eslint-plugin-vitest": "^0.5.4",
        "history": "^5.3.0",
        "jsdom": "27.0.0",
        "msw": "^2.11.3",
        "nswag": "14.6.1",
        "prettier": "^3.6.2",
        "tslib": "^2.8.1",
        "typescript": "^5.9.3",
        "vite": "^7.1.9",
        "vite-bundle-visualizer": "^1.2.1",
        "vite-plugin-compression": "^0.5.1",
        "vite-plugin-eslint": "^1.8.1",
        "vite-plugin-mkcert": "^1.17.8",
        "vite-tsconfig-paths": "^5.1.4",
